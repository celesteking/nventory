<h1>Listing network_interface_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>L2</th>
  </tr>

<% @network_interface_types.each do |network_interface_type| %>
  <tr>
    <td><%=h network_interface_type.name %></td>
    <td><%=h network_interface_type.description %></td>
    <td><%=h network_interface_type.l2_id %></td>
    <td><%= link_to 'Show', network_interface_type %></td>
    <td><%= link_to 'Edit', edit_network_interface_type_path(network_interface_type) %></td>
    <td><%= link_to 'Destroy', network_interface_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New network_interface_type', new_network_interface_type_path %>